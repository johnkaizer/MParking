<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.project.digiparking.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_bookings"
        android:name="com.project.digiparking.ui.Bookings.BookingsFragment"
        android:label="My Parking History"
        tools:layout="@layout/fragment_bookings" />

    <fragment
        android:id="@+id/nav_maps"
        android:name="com.project.digiparking.ui.Maps.MapsFragment"
        android:label="Map"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/nav_car"
        android:name="com.project.digiparking.ui.CarDetails.CarFragment"
        android:label="My Car"
        tools:layout="@layout/fragment_car" />
</navigation>